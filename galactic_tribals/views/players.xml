        <record model="ir.ui.view" id="galactic_tribals.player_form">
            <field name="name">GalacticTribals Player form</field>
            <field name="model">galactic_tribals.player</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <field name="avatar" widget="image"/>
                        <group>
                            <separator string="Personal data"></separator>
                            <group>
                                <field name="name"/>
                                <field name="email"/>
                                <field name="register_date"/>
                                <field name="isActive"/>
                            </group>
                            <separator string="Player data"></separator>
                            <group>
                                <field name="level"/>
                                <field name="battle_points"/>
                                <field name="tribu"/>
                            </group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record model="ir.ui.view" id="galactic_tribals.quick_create_player_form">
            <field name="name">GalacticTribals Player Quick form</field>
            <field name="model">galactic_tribals.player</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="name"/>
                    <field name="email"/>
                    <field name="register_date"/>
                </tree>
            </field>
        </record>
    </data>
